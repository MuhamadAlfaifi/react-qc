{
  "name": "react-qc",
  "version": "0.0.2",
  "description": "React-QC simplifies the creation of reusable data fetching components by abstracting React Query hooks into intuitive component query helpers.",
  "author": "Muhamad Alfaifi (muhamad.alfaifi@gmail.com)",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "watch": "tsc && vite build --watch",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest --watch",
    "publish": "npm run build && npm publish"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^4.36.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "react-error-boundary": "^4.0.11"
  }
}
